  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "json-server": "0.4.2",
    "nodemon": "1.19.4",
    "pino-pretty": "8.1.0",
