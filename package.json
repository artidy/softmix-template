{
  "name": "softmix",
  "version": "1.0.0",
  "description": "Soft Mix backend",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.json",
    "start:dev": "nodemon",
    "debug": "npm run ts",
    "ts": "ts-node",
    "mock:server": "json-server mocks/data.json --port 3123",
    "model:generate": "sequelize model:generate --models-path ./src/models "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artidy/softmix.git"
  },
  "author": "Andrey Pechennikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artidy/softmix/issues"
  },
  "homepage": "https://github.com/artidy/softmix#readme",
  "type": "module",
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "devDependencies": {
    "@types/convict": "6.1.1",
    "@types/convict-format-with-validator": "6.0.2",
    "@types/express": "4.17.13",
    "@types/lozad": "1.16.1",
    "@types/node": "18.0.0",
    "@types/validator": "13.7.4",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "json-server": "0.17.0",
    "nodemon": "1.19.4",
    "pino-pretty": "8.1.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "convict": "6.2.3",
    "convict-format-with-validator": "6.2.0",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-async-handler": "1.2.0",
    "got": "12.1.0",
    "http-status-codes": "2.2.0",
    "inversify": "6.0.1",
    "lozad": "1.16.0",
    "mysql2": "2.3.3",
    "pino": "8.1.0",
    "reflect-metadata": "0.1.13",
